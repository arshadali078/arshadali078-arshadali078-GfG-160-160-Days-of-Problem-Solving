Stock Buy and Sell â€“ Multiple Transaction Allowe
class Solution {
  public:

int n = prices.size();
          int profit = 0; 
    
         for (int i = 0; i < n - 1; i++) {
      
        if (prices[i + 1] > prices[i]) {
            profit += prices[i + 1] - prices[i];
        }
    }
    
    return profit; 
    }
  }
